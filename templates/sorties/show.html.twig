{% extends 'base.html.twig' %}

{% block title %}Sorties{% endblock %}

{% block body %}
    <h1>Sorties</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sorty.id }}</td>
            </tr>
            <tr>
                <th>No_sortie</th>
                <td>{{ sorty.noSortie }}</td>
            </tr>
            <tr>
                <th>Nom</th>
