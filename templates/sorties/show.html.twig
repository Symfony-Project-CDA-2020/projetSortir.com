            <tr>
                <th>Organisateur</th>
                <td>{{ sorty.organisateur }}</td>
            </tr>
            <tr>
                <th>Lieux_no_lieu</th>
                <td>{{ sorty.lieuxNoLieu }}</td>
            </tr>
            <tr>
                <th>Etats_no_etat</th>
                <td>{{ sorty.etatsNoEtat }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('sorties_index') }}">back to list</a>

    <a href="{{ path('sorties_edit', {'id': sorty.id}) }}">edit</a>

    {{ include('sorties/_delete_form.html.twig') }}
